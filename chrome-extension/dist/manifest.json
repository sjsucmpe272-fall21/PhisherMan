{
    "name": "Phisherman",
    "description": "Active phishing detection",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "tabs",
        "storage",
        "webRequest"
    ],
    "host_permissions": [
        "https://*.virustotal.com/*",
        "<all_urls>"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "index.html"
    },
    "options_page": "options.html"
}
